<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <f:entry title="${%Host}" field="host">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%MacAddress}" field="macAddress">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%PingInterval}" field="pingInterval">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%ConnectionTimeout}" field="connectionTimeout">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%AutoSuspend}" field="autoSuspend">
    <f:checkbox/>
  </f:entry>
  <f:entry title="${%SuspendAsSuperuser}" field="suspendAsSuperuser">
    <f:checkbox/>
  </f:entry>
  <f:entry title="${%IgnoreSessionsOnSuspend}" field="ignoreSessionsOnSuspend">
    <f:checkbox/>
  </f:entry>

  <st:include page="config.jelly" class="${descriptor.sshConnectorClass}"/>
</j:jelly>
